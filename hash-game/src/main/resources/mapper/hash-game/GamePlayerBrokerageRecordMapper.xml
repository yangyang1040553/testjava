<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.game.mapper.GamePlayerBrokerageRecordMapper">

    <resultMap type="GamePlayerBrokerageRecord" id="GamePlayerBrokerageRecordResult">
        <result property="id" column="id"/>
        <result property="userId" column="user_id"/>
        <result property="level" column="level"/>
        <result property="walletType" column="wallet_type"/>
        <result property="betAmount" column="bet_amount"/>
        <result property="brokerageType" column="brokerage_type"/>
        <result property="brokerageAmount" column="brokerage_amount"/>
        <result property="childUserId" column="child_user_id"/>
        <result property="status" column="status"/>
        <result property="createTime" column="create_time"/>
    </resultMap>

    <sql id="selectGamePlayerBrokerageRecordVo">
        select id,
               user_id,
               level,
               wallet_type,
               bet_amount,
               brokerage_type,
               brokerage_amount,
               child_user_id,
               status,
               create_time
        from t_game_player_brokerage_record
    </sql>

    <select id="selectGamePlayerBrokerageRecordList" parameterType="GamePlayerBrokerageRecord"
            resultMap="GamePlayerBrokerageRecordResult">
        <include refid="selectGamePlayerBrokerageRecordVo"/>
        <where>
            <if test="userId != null  and userId != ''">and user_id = #{userId}</if>
            <if test="level != null ">and level = #{level}</if>
            <if test="walletType != null  and walletType != ''">and wallet_type = #{walletType}</if>
            <if test="brokerageType != null ">and brokerage_type = #{brokerageType}</if>
            <if test="childUserId != null  and childUserId != ''">and child_user_id = #{childUserId}</if>
            <if test="status != null ">and status = #{status}</if>
            <if test="createTime != null ">and create_time >= #{createTime}</if>
        </where>
        <if test="sort != null "> order by create_time ${sort}</if>
    </select>

    <select id="selectGamePlayerBrokerageRecordById" parameterType="String" resultMap="GamePlayerBrokerageRecordResult">
        <include refid="selectGamePlayerBrokerageRecordVo"/>
        where id = #{id}
    </select>

    <insert id="insertGamePlayerBrokerageRecord" parameterType="GamePlayerBrokerageRecord">
        insert into t_game_player_brokerage_record
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="userId != null">user_id,</if>
            <if test="level != null">level,</if>
            <if test="walletType != null">wallet_type,</if>
            <if test="betAmount != null">bet_amount,</if>
            <if test="brokerageType != null">brokerage_type,</if>
            <if test="brokerageAmount != null">brokerage_amount,</if>
            <if test="childUserId != null">child_user_id,</if>
            <if test="status != null">status,</if>
            <if test="createTime != null">create_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="userId != null">#{userId},</if>
            <if test="level != null">#{level},</if>
            <if test="walletType != null">#{walletType},</if>
            <if test="betAmount != null">#{betAmount},</if>
            <if test="brokerageType != null">#{brokerageType},</if>
            <if test="brokerageAmount != null">#{brokerageAmount},</if>
            <if test="childUserId != null">#{childUserId},</if>
            <if test="status != null">#{status},</if>
            <if test="createTime != null">#{createTime},</if>
        </trim>
    </insert>

    <update id="updateGamePlayerBrokerageRecord" parameterType="GamePlayerBrokerageRecord">
        update t_game_player_brokerage_record
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="level != null">level = #{level},</if>
            <if test="walletType != null">wallet_type = #{walletType},</if>
            <if test="betAmount != null">bet_amount = #{betAmount},</if>
            <if test="brokerageType != null">brokerage_type = #{brokerageType},</if>
            <if test="brokerageAmount != null">brokerage_amount = #{brokerageAmount},</if>
            <if test="childUserId != null">child_user_id = #{childUserId},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteGamePlayerBrokerageRecordById" parameterType="String">
        delete
        from t_game_player_brokerage_record
        where id = #{id}
    </delete>

    <delete id="deleteGamePlayerBrokerageRecordByIds" parameterType="String">
        delete from t_game_player_brokerage_record where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>