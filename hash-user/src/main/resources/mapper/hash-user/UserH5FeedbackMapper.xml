<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.hashuser.mapper.UserH5FeedbackMapper">
    
    <resultMap type="UserH5Feedback" id="UserH5FeedbackResult">
        <result property="id"    column="id"    />
        <result property="items"    column="items"    />
        <result property="content"    column="content"    />
        <result property="ip"    column="ip"    />
        <result property="email"    column="email"    />
        <result property="createTime"    column="create_time"    />
    </resultMap>

    <sql id="selectUserH5FeedbackVo">
        select id, items, content, ip, email, create_time from t_user_h5_feedback
    </sql>

    <select id="selectUserH5FeedbackList" parameterType="UserH5Feedback" resultMap="UserH5FeedbackResult">
        <include refid="selectUserH5FeedbackVo"/>
        <where>  
            <if test="id != null "> and id = #{id}</if>
            <if test="items != null  and items != ''"> and items = #{items}</if>
            <if test="content != null  and content != ''"> and content = #{content}</if>
            <if test="ip != null  and ip != ''"> and ip = #{ip}</if>
            <if test="email != null  and email != ''"> and email = #{email}</if>
            <if test="createTime != null "> and create_time >= #{createTime}</if>
        </where>
    </select>
    
    <select id="selectUserH5FeedbackById" parameterType="Long" resultMap="UserH5FeedbackResult">
        <include refid="selectUserH5FeedbackVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertUserH5Feedback" parameterType="UserH5Feedback" useGeneratedKeys="true" keyProperty="id">
        insert into t_user_h5_feedback
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="items != null">items,</if>
            <if test="content != null">content,</if>
            <if test="ip != null">ip,</if>
            <if test="email != null">email,</if>
            <if test="createTime != null">create_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="items != null">#{items},</if>
            <if test="content != null">#{content},</if>
            <if test="ip != null">#{ip},</if>
            <if test="email != null">#{email},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateUserH5Feedback" parameterType="UserH5Feedback">
        update t_user_h5_feedback
        <trim prefix="SET" suffixOverrides=",">
            <if test="items != null">items = #{items},</if>
            <if test="content != null">content = #{content},</if>
            <if test="ip != null">ip = #{ip},</if>
            <if test="email != null">email = #{email},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteUserH5FeedbackById" parameterType="Long">
        delete from t_user_h5_feedback where id = #{id}
    </delete>

    <delete id="deleteUserH5FeedbackByIds" parameterType="String">
        delete from t_user_h5_feedback where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>