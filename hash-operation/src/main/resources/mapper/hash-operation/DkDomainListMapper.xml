<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.operation.mapper.DkDomainListMapper">

    <resultMap type="DkDomainList" id="DkDomainListResult">
        <result property="id" column="id"/>
        <result property="domainType" column="domain_type"/>
        <result property="domain" column="domain"/>
        <result property="createTime" column="create_time"/>
    </resultMap>

    <sql id="selectDkDomainListVo">
        select id, domain_type, domain, create_time
        from dk_domain_list
    </sql>

    <select id="selectDkDomainListList" parameterType="DkDomainList" resultMap="DkDomainListResult">
        <include refid="selectDkDomainListVo"/>
        <where>
            <if test="id != null ">and id = #{id}</if>
            <if test="domainType != null  and domainType != ''">and domain_type = #{domainType}</if>
            <if test="domain != null  and domain != ''">and domain = #{domain}</if>
            <if test="createTime != null ">and create_time = #{createTime}</if>
        </where>
    </select>

    <select id="selectDkDomainListById" parameterType="Long" resultMap="DkDomainListResult">
        <include refid="selectDkDomainListVo"/>
        where id = #{id}
    </select>

    <insert id="insertDkDomainList" parameterType="DkDomainList" useGeneratedKeys="true" keyProperty="id">
        insert into dk_domain_list
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="domainType != null and domainType != ''">domain_type,</if>
            <if test="domain != null and domain != ''">domain,</if>
            <if test="createTime != null">create_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="domainType != null and domainType != ''">#{domainType},</if>
            <if test="domain != null and domain != ''">#{domain},</if>
            <if test="createTime != null">#{createTime},</if>
        </trim>
    </insert>

    <update id="updateDkDomainList" parameterType="DkDomainList">
        update dk_domain_list
        <trim prefix="SET" suffixOverrides=",">
            <if test="domainType != null and domainType != ''">domain_type = #{domainType},</if>
            <if test="domain != null and domain != ''">domain = #{domain},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteDkDomainListById" parameterType="Long">
        delete
        from dk_domain_list
        where id = #{id}
    </delete>

    <delete id="deleteDkDomainListByIds" parameterType="String">
        delete from dk_domain_list where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>