<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.operation.mapper.DkOssListMapper">

    <resultMap type="DkOssList" id="DkOssListResult">
        <result property="id" column="id"/>
        <result property="ossType" column="oss_type"/>
        <result property="bucketName" column="bucket_name"/>
        <result property="accessId" column="access_id"/>
        <result property="accessKey" column="access_key"/>
        <result property="uploadAddr" column="upload_addr"/>
        <result property="accessAddr" column="access_addr"/>
        <result property="satus" column="satus"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateBy" column="update_by"/>
    </resultMap>

    <sql id="selectDkOssListVo">
        select id,
               oss_type,
               bucket_name,
               access_id,
               access_key,
               upload_addr,
               access_addr,
               satus,
               create_time,
               update_time,
               update_by
        from dk_oss_list
    </sql>


    <select id="selectDKOSSbyName" parameterType="String" resultMap="DkOssListResult">
        <include refid="selectDkOssListVo"/>
        <where>
            <if test="bucket_name != null and bucket_name!=''">bucket_name in( ${ossType})</if>
        </where>
    </select>

    <select id="selectDkOssListList" parameterType="DkOssList" resultMap="DkOssListResult">
        <include refid="selectDkOssListVo"/>
        <where>
            <if test="ossType != null ">and oss_type = #{ossType}</if>
            <if test="bucketName != null  and bucketName != ''">and bucket_name like concat('%', #{bucketName}, '%')
            </if>
            <if test="accessId != null  and accessId != ''">and access_id = #{accessId}</if>
            <if test="accessKey != null  and accessKey != ''">and access_key = #{accessKey}</if>
            <if test="uploadAddr != null  and uploadAddr != ''">and upload_addr = #{uploadAddr}</if>
            <if test="accessAddr != null  and accessAddr != ''">and access_addr = #{accessAddr}</if>
            <if test="satus != null ">and satus = #{satus}</if>
            <if test="createTime != null ">and create_time = #{createTime}</if>
        </where>
    </select>

    <select id="selectDkOssListById" parameterType="Long" resultMap="DkOssListResult">
        <include refid="selectDkOssListVo"/>
        where id = #{id}
    </select>

    <insert id="insertDkOssList" parameterType="DkOssList" useGeneratedKeys="true" keyProperty="id">
        insert into dk_oss_list
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="ossType != null">oss_type,</if>
            <if test="bucketName != null">bucket_name,</if>
            <if test="accessId != null">access_id,</if>
            <if test="accessKey != null">access_key,</if>
            <if test="uploadAddr != null">upload_addr,</if>
            <if test="accessAddr != null">access_addr,</if>
            <if test="satus != null">satus,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="updateBy != null">update_by,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="ossType != null">#{ossType},</if>
            <if test="bucketName != null">#{bucketName},</if>
            <if test="accessId != null">#{accessId},</if>
            <if test="accessKey != null">#{accessKey},</if>
            <if test="uploadAddr != null">#{uploadAddr},</if>
            <if test="accessAddr != null">#{accessAddr},</if>
            <if test="satus != null">#{satus},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
        </trim>
    </insert>

    <update id="updateDkOssList" parameterType="DkOssList">
        update dk_oss_list
        <trim prefix="SET" suffixOverrides=",">
            <if test="ossType != null">oss_type = #{ossType},</if>
            <if test="bucketName != null">bucket_name = #{bucketName},</if>
            <if test="accessId != null">access_id = #{accessId},</if>
            <if test="accessKey != null">access_key = #{accessKey},</if>
            <if test="uploadAddr != null">upload_addr = #{uploadAddr},</if>
            <if test="accessAddr != null">access_addr = #{accessAddr},</if>
            <if test="satus != null">satus = #{satus},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteDkOssListById" parameterType="Long">
        delete
        from dk_oss_list
        where id = #{id}
    </delete>

    <delete id="deleteDkOssListByIds" parameterType="String">
        delete from dk_oss_list where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>